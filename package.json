{
  "name": "@web-project/react",
  "version": "1.7.3",
  "author": "doasync",
  "description": "Create a project with Webpack 4, Babel 7, React, Flow, ESLint",
  "license": "MIT",
  "homepage": "https://github.com/doasync/web-project-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/doasync/web-project-react.git"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.1.0"
  },
  "keywords": [
    "webpack",
    "react",
    "babel",
    "eslint",
    "flow",
    "create-react-app",
    "web-project"
  ],
  "bin": "./cli/exec.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "build": "npm run build-js",
    "build-js": "webpack --config config/webpack.prod.js --progress",
    "build-info": "webpack --config config/webpack.prod.js --profile --display=normal --json > config/info/webpack-stats.json",
    "postbuild-info": "webpack-bundle-analyzer config/info/webpack-stats.json dist"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "yargs": "^11",
    "fs-extra": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-flow": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-bundle": "^2.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "browserslist": [
    "Chrome >= 58",
    "Firefox >= 56",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Edge >= 16"
  ]
}
