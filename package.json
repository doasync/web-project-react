{
  "name": "@web-project/react",
  "version": "2.0.0",
  "author": "doasync",
  "description": "Create a project with Webpack 4, Babel 7, React, Flow, ESLint, Jest",
  "license": "MIT",
  "homepage": "https://github.com/doasync/web-project-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/doasync/web-project-react.git"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.1.0"
  },
  "keywords": [
    "webpack",
    "react",
    "babel",
    "eslint",
    "flow",
    "create-react-app",
    "web-project"
  ],
  "bin": "./cli/exec.js",
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:js": "webpack --config webpack/webpack.prod.js --progress",
    "build:css": "webpack --config webpack/webpack.css.js",
    "build:serve": "cd dist && http-server -a localhost",
    "build:info": "webpack --config webpack/webpack.prod.js --profile --display=normal --json > webpack/info/webpack-stats.json",
    "postbuild:info": "webpack-bundle-analyzer webpack/info/webpack-stats.json dist -h localhost -O",
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint '+(**/*.css|**/*.scss|**/*.js|**/*.html)'",
    "lint:js": "eslint .",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js'",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "check": "npm run lint && npm run flow:coverage && npm run test:coverage && npm run build:info"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "fbjs": "^0.8.17",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "yargs": "^11",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "del-webpack-plugin": "^1.0.6",
    "dotenv-webpack": "^1.5.7",
    "error-overlay-webpack-plugin": "^0.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-bundle": "^2.0.4",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.77.0",
    "flow-coverage-report": "^0.5.0",
    "flow-typed": "^2.5.1",
    "flow-webpack-plugin": "^1.2.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "mini-css-extract-plugin": "^0.4.1",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.1",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^4.0.0",
    "postcss-jsx": "^0.32.0",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^3.0.0",
    "postcss-normalize": "^6.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-scss": "^2.0.0",
    "postcss-syntax": "^0.32.0",
    "postcss-url": "^7.3.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-no-unsupported-browser-features": "^3.0.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "browserslist": [
    "Chrome >= 58",
    "Firefox >= 56",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Edge >= 16"
  ]
}
